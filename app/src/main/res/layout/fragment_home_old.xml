<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/W_m_H_m"
    android:background="@drawable/main_background"
    android:orientation="vertical" >

    <!--
        <ViewStub
        android:id="@+id/vsHeader"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inflatedId="@+id/vsHeader"
        android:layout="@layout/header_title" />
    -->

    <LinearLayout
        style="@style/W_m_H_m"
        android:gravity="center"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/imageView1"
            style="@style/W_w_H_w"
            android:background="@drawable/home_bt1"
            android:paddingTop="60dp" />

        <LinearLayout
            android:id="@+id/relative1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="vertical"
            android:visibility="gone" >

            <TextView
                android:id="@+id/textView1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:text="Clearance" />

            <ImageView
                android:id="@+id/imageLine"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/seperate_line" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:text="Sector Transaction" />
        </LinearLayout>

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:background="@drawable/logo_inside" />

        <ImageView
            android:id="@+id/imageView2"
            style="@style/W_w_H_w"
            
            android:background="@drawable/home_bt2" />
    </LinearLayout>

</LinearLayout>